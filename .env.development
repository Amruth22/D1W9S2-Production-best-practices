# Development Environment Configuration
ENVIRONMENT=development
DEBUG=true

# Database
DATABASE_URL=student_grades_dev.db

# Performance Settings
CACHE_SIZE=500
BATCH_SIZE=50
QUERY_TIMEOUT=10

# Monitoring & Alerts
ALERT_RESPONSE_TIME_MS=500
