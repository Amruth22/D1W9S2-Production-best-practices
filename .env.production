# Production Environment Configuration
ENVIRONMENT=production
DEBUG=false

# Database
DATABASE_URL=student_grades_prod.db

# Performance Settings
CACHE_SIZE=2000
BATCH_SIZE=200
QUERY_TIMEOUT=30

# Monitoring & Alerts (Stricter in production)
ALERT_RESPONSE_TIME_MS=200
ALERT_CACHE_HIT_RATE=0.80
ALERT_MEMORY_MB=1000

# Cost Tracking (Production rates)
COST_PER_DB_QUERY=0.0002
COST_PER_100_API_CALLS=0.002

# Logging
LOG_LEVEL=INFO