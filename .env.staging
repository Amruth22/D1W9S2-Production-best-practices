# Staging Environment Configuration
ENVIRONMENT=staging
DEBUG=false

# Database
DATABASE_URL=student_grades_staging.db

# Performance Settings
CACHE_SIZE=1000
BATCH_SIZE=100
QUERY_TIMEOUT=20

# Monitoring & Alerts (Between dev and prod)
ALERT_RESPONSE_TIME_MS=300
ALERT_CACHE_HIT_RATE=0.70
ALERT_MEMORY_MB=500

# Cost Tracking (Staging rates)
COST_PER_DB_QUERY=0.00015
COST_PER_100_API_CALLS=0.0015

# Logging
LOG_LEVEL=INFO